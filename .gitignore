# Ignore node_modules and build output in the frontend folder
/frontend/node_modules
/frontend/build

# Ignore log files
/frontend/npm-debug.log*
/frontend/yarn-debug.log*
/frontend/yarn-error.log*

# Ignore environment variables
/frontend/.env
/frontend/.env.local
/frontend/.env.development
/frontend/.env.test
/frontend/.env.production

# Ignore public environment configuration
/frontend/public/env-config.js

# Ignore cache and coverage data
/frontend/.next
/frontend/.cache
/frontend/coverage
/frontend/.nyc_output

# Ignore IDE or editor-specific files
/frontend/.vscode/
/frontend/.idea/
/frontend/*.sublime-project
/frontend/*.sublime-workspace

# Ignore miscellaneous files
/frontend/*.DS_Store
/frontend/.eslintcache
/frontend/*.log
